    	(lambda (x::(bitvector 8))
      (bv-extract 7 0 (bv-lshr othermem (bv-zero-extend x 2040))))))
    ; Create the input "state" by concatenating all of the inputs.
    (let ((state0   (bv-concat r7 r6 r5 r4 r3 r2 r1 r0)))
    ; Set initial EIP to zero.
