    (ite (= x 0b110) (bv-concat (bv-extract 7 7 fs) y (bv-extract 5 0 fs))
                     (bv-concat (bv-extract 7 7 fs) y (bv-extract 5 0 fs)))))))))))
    (bv-concat newfs rs)
